{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "workflow": {
        "collect": {
            "metrics": {
                "/staples/procfs/procstat/*/cpu_usage" : {}
            },
            "config": {
            },
            "process": [
                {
                    "plugin_name": "passthru",
                    "publish": [
                        {
                            "plugin_name": "file",
                            "config": {
                                "file": "/tmp/snap-file.out"
                            }
                        }
                    ],
                    "config": null
                }
            ],
            "publish": null
        }
    }
}
